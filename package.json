{
  "name": "markdown-poll",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "up": "docker compose -f environment/dev/docker-compose.yml up -d",
    "down": "docker compose -f environment/dev/docker-compose.yml down",
    "migration:create": "node-pg-migrate --migrations-dir src/infra/migrations create",
    "migration:run": "node-pg-migrate up --envPath ./.env -m src/infra/migrations/",
    "migration:dry-run": "node-pg-migrate up --dry-run --envPath ./.env -m src/infra/migrations",
    "migration:seed": "node -r dotenv-expand/config src/infra/scripts/seed-database.js",
    "vercel-build": "yum install gcc-c++ cairo-devel libjpeg-turbo-devel pango-devel giflib-devel -y"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "async-retry": "^1.3.3",
    "canvas": "~2.8.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "heroicons": "^2.0.18",
    "joi": "^17.11.0",
    "jsdom": "19.0.0",
    "next": "13.5.5",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "pino": "^8.16.0",
    "react": "^18",
    "react-dom": "^18",
    "snakeize": "^0.1.0",
    "uuid": "^9.0.1",
    "validate-color": "^2.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.5",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "husky": "^8.0.3",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
